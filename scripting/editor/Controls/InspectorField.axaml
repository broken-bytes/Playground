<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PlaygroundEditor.Controls.InspectorField">
    <Grid Name="Content" RowDefinitions="24, 24" HorizontalAlignment="Stretch">
        <TextBlock Name = "FieldTitle" Grid.Row="0" FontFamily="{StaticResource Rubik}" FontWeight="Bold"/>
        <StackPanel Grid.Row="1" Name = "FieldContent" HorizontalAlignment="Stretch"></StackPanel>
    </Grid>
</UserControl>

